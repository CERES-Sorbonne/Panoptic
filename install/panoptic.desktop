[Desktop Entry]
Version=1.0
Type=Application
Name=Panoptic
Comment=Panoptic
Terminal=true
Icon=PANOPTIC_ICON_PATH
Exec=/usr/local/bin/start-panoptic

[Desktop Action start-only]
Name=Start Panoptic
Exec=/usr/local/bin/start-panoptic -s

[Desktop Action reinstall]
Name=Reinstall Panoptic
Exec=/usr/local/bin/start-panoptic -r

[Desktop Action uninstall]
Name=Uninstall Panoptic
Exec=/usr/local/bin/start-panoptic -u
